{
  "name": "Britescript Dark",
  "type": "dark",
  "colors": {
    "editor.background": "#1e1e1e",
    "editor.foreground": "#d4d4d4",
    "activityBar.background": "#2d2d30",
    "sideBar.background": "#252526",
    "statusBar.background": "#007acc",
    "statusBar.foreground": "#ffffff"
  },
  "tokenColors": [
    {
      "name": "Comment",
      "scope": [
        "comment"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#6A9955"
      }
    },
    {
      "name": "Britescript Keywords",
      "scope": [
        "keyword.control.britescript",
        "storage.type.britescript"
      ],
      "settings": {
        "foreground": "#C586C0",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Struct Name",
      "scope": [
        "entity.name.type.struct.britescript"
      ],
      "settings": {
        "foreground": "#4EC9B0",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Trait Name",
      "scope": [
        "entity.name.type.trait.britescript"
      ],
      "settings": {
        "foreground": "#4FC1FF",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Pipe Operator",
      "scope": [
        "keyword.operator.pipe.britescript"
      ],
      "settings": {
        "foreground": "#FF6B35",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Pipe Functions",
      "scope": [
        "entity.name.function.pipe.britescript"
      ],
      "settings": {
        "foreground": "#DCDCAA"
      }
    },
    {
      "name": "Let Binding Variable",
      "scope": [
        "variable.other.constant.britescript"
      ],
      "settings": {
        "foreground": "#79C0FF",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Generic Types",
      "scope": [
        "entity.name.type.generic.britescript"
      ],
      "settings": {
        "foreground": "#B5CEA8"
      }
    },
    {
      "name": "Struct Fields",
      "scope": [
        "variable.other.property.britescript"
      ],
      "settings": {
        "foreground": "#9CDCFE"
      }
    },
    {
      "name": "Trait Methods",
      "scope": [
        "entity.name.function.trait.britescript"
      ],
      "settings": {
        "foreground": "#DCDCAA"
      }
    },
    {
      "name": "String",
      "scope": [
        "string"
      ],
      "settings": {
        "foreground": "#CE9178"
      }
    },
    {
      "name": "Number",
      "scope": [
        "constant.numeric"
      ],
      "settings": {
        "foreground": "#B5CEA8"
      }
    },
    {
      "name": "TypeScript Keywords",
      "scope": [
        "keyword.control.typescript",
        "storage.type.typescript"
      ],
      "settings": {
        "foreground": "#569CD6"
      }
    },
    {
      "name": "Types",
      "scope": [
        "support.type.typescript",
        "entity.name.type.britescript"
      ],
      "settings": {
        "foreground": "#4EC9B0"
      }
    },
    {
      "name": "Constants",
      "scope": [
        "constant.language"
      ],
      "settings": {
        "foreground": "#569CD6"
      }
    }
  ]
}